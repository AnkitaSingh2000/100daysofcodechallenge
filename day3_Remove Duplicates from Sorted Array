class Solution {
    public int removeDuplicates(int[] nums) {
        //Arrays.sort(nums);
        int i,p=0;
        for(i=0;i<nums.length-1;i++){
            if(nums[i]!=nums[i+1]){
                nums[p]=nums[i];
                p++;
            }
        }
        nums[p]=nums[i];
        return p+1;
            }
        }
