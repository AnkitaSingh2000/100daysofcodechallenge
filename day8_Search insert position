class Solution {
    public int searchInsert(int[] nums, int target) {
        int mid=0,start=0,end=nums.length;
        if(nums[nums.length-1]<target)
                return nums.length;
                while (start <=end){
                    mid=(start+end)/2;
                    if(nums[mid]==target)
                        return mid;
                    if(nums[mid]<target)
                        start =mid+1;
                    if(nums[mid]>target)
                       end=mid-1;
                }
                if(nums[mid]<target)
                   mid=mid+1;
                return mid;
                }
                
        }
